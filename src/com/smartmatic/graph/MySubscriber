package com.smartmatic.graph;

public class MySubscriber extends Subscriber {

  private Session session;
  
  private String regionName;
  
  public MySubscriber(Session session) {
      this.session = session;
  }
  
  public void onComplete() {}
  
  public void onNext() {
      session.getBasicRemote().sendMessage();
  }
  public void onError() {}
  
  @override
  public boolean equals(Object obj) {
    if (obj instanceof MySubscriber ) {
        return this.regionName.compareIgnoreCase(((MySubscriber)obj).regionName);
    }
    
    return false;
  }
  
}
